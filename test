node {
	stage("checkout") {
	    checkout([$class: 'GitSCM', branches: [[name: '*/master']], doGenerateSubmoduleConfigurations: false, extensions: [], submoduleCfg: [], userRemoteConfigs: [[url: 'https://github.com/ktianc/test_project.git']]])
	}
	stage("maven pack") {
	    echo "maven pack....."
	}
	stage("deploy") {
	    echo "deploy project......"
	}
}